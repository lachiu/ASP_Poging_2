@model VoorraadSysteem.ViewModels.DeleteVatPercentageViewModel

@{
	ViewData["Title"] = Model.Percentage + "% verwijderen";
}

<h1>@Model.Percentage% verwijderen</h1>
<h4>Ben je zeker dat je @Model.Percentage% wilt verwijderen?</h4>
<hr />
<form asp-action="Delete">
	<input type="hidden" asp-for="Id" />
	<input type="submit" value="Delete" class="btn btn-primary" /> | <a asp-action="Index">Ga terug naar de lijst</a>
</form>